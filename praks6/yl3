#!/bin/bash
#
osch=0
echo "1. Unix (Sun Os)"
echo "2. Linux (Red Hat)"
echo -n "Vali enda operatsioonis체steem [1 v천i 2]? "
read osch
if [ $osch -eq 1 ] ; then
     echo "Sa valisid (Sun Os) operatsioonis체steemi"
else #### nested if i.e. if within if ######
       if [ $osch -eq 2 ] ; then
             echo "Sa valisid (Red Hat) operatsioonis체steemi"
       else
             echo "Kas sulle ei meeldi Unix/Linux OS."
       fi
fi
#

